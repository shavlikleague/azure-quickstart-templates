{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"_artifactsLocation": {
			"value": "https://raw.githubusercontent.com/shavlikleague/azure-quickstart-templates/master/openshift-origin-rhel/"
		},
		"masterVmSize": {
			"value": "Standard_DS3_v2"
		},
		"nodeVmSize": {
			"value": "Standard_DS3_v2"
		},
		"osImage": {
			"value": "centos"
		},
		"openshiftMasterHostname": {
			"value": "os-master01"
		},
		"openshiftMasterPublicIpDnsLabelPrefix": {
			"value": "GEN-UNIQUE"
		},
		"nodeLbPublicIpDnsLabelPrefix": {
			"value": "GEN-UNIQUE"
		},
		"nodePrefix": {
			"value": "os-node"
		},
		"nodeInstanceCount": {
			"value": 3
		},
		"adminUsername": {
                   "type": "string",
                   "metadata": {
                     "description": "User name for the OS Master Admin."
                     }
                },
                "adminPassword": {
                   "type": "securestring",
                   "metadata": {
                     "description": "Password for the OS Master Admin."
                     }
                },
		"sshPublicKey": {
			"value": "GEN-SSH-PUB-KEY"
		},
		"subscriptionId": {
			"value": "changeme"
		},
		"keyVaultResourceGroup": {
			"value": "changeme"
		},
		"keyVaultName": {
			"value": "changeme"
		},
		"keyVaultSecret": {
			"value": "changeme"
		},
		"defaultSubDomainType": {
			"value": "xipio"
		},
		"defaultSubDomain": {
			"value": "changeme"
		}
	}
}
